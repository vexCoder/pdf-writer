{
    "name": "pdf-writer",
    "version": "1.0.0",
    "description": "Converts csv to PDF",
    "main": "index.js",
    "scripts": {
        "build": "tsc",
        "watch": "tsc --watch --preserveWatchOutput",
        "run:dev": "cross-env NODE_ENV=development nodemon -q --inspect dist/index.js",
        "run:start": "cross-env NODE_ENV=production node dist/index.js",
        "predev:test": "yarn install & yarn lint:fix",
        "dev": "concurrently -p \"[{time}] [{name}]\" -t HH:mm:ss -c red,green -n WATCH,DEV \"yarn watch\" \"yarn run:dev\"",
        "start": "yarn build & concurrently -p \"[{time}] [{name}]\" -t HH:mm:ss -c green -n PROD \"yarn run:start\"",
        "read:pdf": "nodemon scripts/readPDF.js",
        "lint": "eslint src/*",
        "lint:fix": "eslint --fix src/**/*.ts"
    },
    "nodemonConfig": {
        "ignore": [
            "temp/*",
            "db/*",
            "exports/*",
            "templates/*",
            "configs/*"
        ]
    },
    "dependencies": {
        "@types/multer": "^1.4.5",
        "@types/node-fetch": "^2.5.8",
        "@types/papaparse": "^5.2.5",
        "archiver": "^5.3.0",
        "axios": "^0.21.1",
        "body-parser": "^1.19.0",
        "compression": "^1.7.4",
        "cookie-parser": "^1.4.5",
        "copyfiles": "^2.4.1",
        "cors": "^2.8.5",
        "dayjs": "^1.10.4",
        "dotenv": "^8.2.0",
        "express": "^4.17.1",
        "express-session": "^1.17.1",
        "fs-extra": "^9.1.0",
        "google-auth-library": "^7.0.2",
        "googleapis": "^67.1.1",
        "helmet": "^4.4.1",
        "lodash": "^4.17.21",
        "lowdb": "^1.0.0",
        "lowdb-session-store": "^1.1.1",
        "multer": "^1.4.2",
        "nanoid": "^3.1.20",
        "node-fetch": "^2.6.1",
        "node-schedule": "^2.0.0",
        "papaparse": "^5.3.0",
        "pdf-lib": "^1.16.0",
        "readline": "^1.3.0"
    },
    "devDependencies": {
        "@types/archiver": "^5.1.0",
        "@types/axios": "^0.14.0",
        "@types/body-parser": "^1.19.0",
        "@types/compression": "^1.7.0",
        "@types/cookie-parser": "^1.4.2",
        "@types/copyfiles": "^2.4.0",
        "@types/cors": "^2.8.10",
        "@types/dotenv": "^8.2.0",
        "@types/express": "^4.17.11",
        "@types/express-session": "^1.17.3",
        "@types/fs-extra": "^9.0.7",
        "@types/helmet": "^4.0.0",
        "@types/lodash": "^4.14.168",
        "@types/lowdb": "^1.0.9",
        "@types/node": "^14.14.31",
        "@types/node-schedule": "^1.3.1",
        "@typescript-eslint/eslint-plugin": "^4.15.2",
        "@typescript-eslint/parser": "^4.15.2",
        "concurrently": "^6.0.0",
        "cross-env": "^7.0.3",
        "eslint": "^7.21.0",
        "eslint-config-airbnb-base": "^14.2.1",
        "eslint-config-prettier": "^8.1.0",
        "eslint-plugin-import": "^2.22.1",
        "eslint-plugin-prettier": "^3.3.1",
        "nodemon": "^2.0.7",
        "prettier": "^2.2.1",
        "typescript": "^4.2.2"
    },
    "keywords": [],
    "author": "",
    "license": "ISC"
}